using System;
using System.Collections.Generic;

namespace program_2
{
   public class Program
    {
       public static void Main()
        {
            Console.WriteLine("Enter a Number");
            var input = Convert.ToInt32(Console.ReadLine());
            var number = input * 2;

            if (input % 2 != 0) 
            {
               
                for (var i = 1; i <= number; i += 2) 
                    Console.Write(i );
            }
      

        }
    }
}
